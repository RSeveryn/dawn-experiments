    const svgContainer = document.querySelector('.section-{{ section.id }}-bg-container');
    const animationSpeed = parseFloat(getComputedStyle(svgContainer).getPropertyValue('--animation-speed'));

    document.querySelectorAll('path').forEach((path) => {
      const maxDelay = animationSpeed * 0.5;
      const randomDelay = (Math.random() * maxDelay).toFixed(1) + 's';
      path.style.setProperty('--animation-delay', randomDelay);
    });
  </script>
