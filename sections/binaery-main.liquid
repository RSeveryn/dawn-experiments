<style>
    {% comment %}
      Animation for the background image
    {% endcomment %}
    .section-{{ section.id }}-bg-container {
        overflow: hidden;
        --animation-speed: {{ section.settings.bg_animation_speed }}s;
    }
    .section-{{ section.id }}-bg-container svg {
        position: absolute;
        z-index: -1;
        top: 0;
        left: -43%;
        width: 128%;
        height: 113%;

        @media (min-width: 744px) {
            top: 50%;
            transform: translateY(-50%);
            left: -10%;
            width: 100%;
        }

        @media (min-width: 1280px) {
            top: 50%;
            transform: translateY(-50%);
            left: -20%;
            width: 100%;
        }
    }
    .section-{{ section.id }}-bg-container path {
        opacity: 0;
        transform: translateX(-100px);
        animation: fadeIn var(--animation-speed) ease forwards;
        animation-delay: var(--animation-delay);
    }

    @keyframes fadeIn {
        to {
            opacity: 1;
            transform: translateX(0);
        }
    }
</style>

{%- capture cssVars -%}
  --section-mobile-padding-top: {{ section.settings.mobile_padding_top }}px;
  --section-mobile-padding-right: {{ section.settings.mobile_padding_right }}px;
  --section-mobile-padding-bottom: {{ section.settings.mobile_padding_bottom }}px;
  --section-mobile-padding-left: {{ section.settings.mobile_padding_left }}px;
  --section-tablet-padding-top: {{ section.settings.tablet_padding_top }}px;
  --section-tablet-padding-right: {{ section.settings.tablet_padding_right }}px;
  --section-tablet-padding-bottom: {{ section.settings.tablet_padding_bottom }}px;
  --section-tablet-padding-left: {{ section.settings.tablet_padding_left }}px;
  --section-desktop-padding-top: {{ section.settings.desktop_padding_top }}px;
  --section-desktop-padding-right: {{ section.settings.desktop_padding_right }}px;
  --section-desktop-padding-bottom: {{ section.settings.desktop_padding_bottom }}px;
  --section-desktop-padding-left: {{ section.settings.desktop_padding_left }}px;
{%- endcapture -%}

<div class="relative">
  <div class="2xl:container 2xl:mx-auto b-section-paddings xl:flex xl:flex-row-reverse xl:items-center xl:justify-between" style="{{- cssVars | strip_newlines | replace: ' ', '' -}}">
    <div class="relative mx-auto pt-10 max-w-[315px] md:max-w-xl md:pt-28 xl:flex-[0_0_578px] xl:mr-0">
      {%- liquid
        assign product = section.settings.product
        assign variant = product.selected_or_first_available_variant
      -%}

      <div
          data-variant-image-wrapper
          class="absolute left-0 top-0 shadow-[0_30.53px_30.53px_-12.21px_rgba(50,8,106,0.28)] w-1/2 aspect-[0.8/1]
          rounded-lg overflow-hidden md:w-[57.7%] transition-transform duration-75 ease-linear"
      >
        {%- for var in product.variants -%}
          <div {% unless forloop.first %}hidden {%endunless%} data-image-for-variant="{{ var.id }}">
            {%-
                render 'image-with-placeholder',
                image: var.image,
                width: 996,
                widths: '332w, 99w',
                sizes: '(min-width: 750px) 332px, 156px',
                class: 'w-full h-full object-cover block'
            -%}
          </div>
        {%- endfor -%}
      </div>

      <product-form2 class="block ml-auto relative z-1 p-4 rounded-xl bg-white shadow-b-product-card w-[65%]
      md:text-base md:py-5 md:px-[30px]">
        <h2 class="font-bold bg-gr-1 bg-clip-text text-transparent text-[1.2em] leading-[1.1] md:text-[38px] md:leading-[1.3]">
          {{ product.title }}
        </h2>
        <p class="text-[0.75em] leading-[1.33] line-clamp-4 mt-3 md:mt-4 md:text-base md:line-clamp-[8] md:leading-[1.6]">
          {{ product.description | strip_html }}
        </p>

        <variant-picker2 data-current-color="{{ variant.options.first }}" data-current-variant="{{ variant.id }}" class="block mt-4 md:mt-[30px]">
          <p class="text-body-2 text-[0.875em] md:text-base">
            Color: <span data-color-label>{{ variant.options.first }}</span>
          </p>
          <div class="flex items-center gap-x-1.5 mt-2 md:mt-2.5">
            {% for option in product.options_by_name['color'].values %}
              <div class="w-[12%] aspect-square">
                <input
                    id="color-{{ option }}"
                    type="radio"
                    name="color"
                    value="{{ option }}"
                    class="hidden peer"
                    data-color-radio
                    {% if option == variant.options.first %}checked{% endif %}
                >
                <label
                    for="color-{{ option }}"
                    class="block w-full aspect-square p-[10%] rounded-full overflow-hidden border border-[#CCD6E0]
                  peer-checked:border-black hover:cursor-pointer"
                >
                  <span class="block w-full h-full rounded-full" style="background: rgb({{ option.swatch.color.rgb }})"></span>
                </label>
              </div>
            {% endfor %}
          </div>
        </variant-picker2>

        <button data-submit class="mt-4 b-btn b-primary-btn w-full flex b-btn-atc-responsive md:mt-10 md:mb-2.5 relative">
          <svg data-loader="off" class="absolute data-[loader=off]:hidden data-[loader=on]:block w-5 text-gray-200 animate-spin dark:text-gray-600 fill-blue-600" viewBox="0 0 100 101" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M100 50.5908C100 78.2051 77.6142 100.591 50 100.591C22.3858 100.591 0 78.2051 0 50.5908C0 22.9766 22.3858 0.59082 50 0.59082C77.6142 0.59082 100 22.9766 100 50.5908ZM9.08144 50.5908C9.08144 73.1895 27.4013 91.5094 50 91.5094C72.5987 91.5094 90.9186 73.1895 90.9186 50.5908C90.9186 27.9921 72.5987 9.67226 50 9.67226C27.4013 9.67226 9.08144 27.9921 9.08144 50.5908Z" fill="currentColor"/>
            <path d="M93.9676 39.0409C96.393 38.4038 97.8624 35.9116 97.0079 33.5539C95.2932 28.8227 92.871 24.3692 89.8167 20.348C85.8452 15.1192 80.8826 10.7238 75.2124 7.41289C69.5422 4.10194 63.2754 1.94025 56.7698 1.05124C51.7666 0.367541 46.6976 0.446843 41.7345 1.27873C39.2613 1.69328 37.813 4.19778 38.4501 6.62326C39.0873 9.04874 41.5694 10.4717 44.0505 10.1071C47.8511 9.54855 51.7191 9.52689 55.5402 10.0491C60.8642 10.7766 65.9928 12.5457 70.6331 15.2552C75.2735 17.9648 79.3347 21.5619 82.5849 25.841C84.9175 28.9121 86.7997 32.2913 88.1811 35.8758C89.083 38.2158 91.5421 39.6781 93.9676 39.0409Z" fill="currentFill"/>
          </svg>

          <span>Add to Cart</span>
          <span class="inline-block w-[2px] h-[1em] bg-current mx-[7px] md:mx-3"></span>
          <span data-variant-price>${{ variant.price | money_without_currency | split: '.' | first }}</span>
        </button>

        <div data-status-message hidden class="bg-[#E0F0FF] w-1/2 absolute left-1/2 -translate-x-1/2 top-full py-1 px-2
        rounded-lg shadow-md text-center text-xs md:text-base md:top-[98%]">
          <div class="w-0 h-0 absolute left-1/2 -translate-x-1/2 -top-2.5
            border-l-[10px] border-l-transparent
            border-b-[10px] border-b-[#E0F0FF]
            border-r-[10px] border-r-transparent">
          </div>
          <span></span>
        </div>
      </product-form2>

      {% render 'icon', name: 'sm-dollar', class: 'w-[3.5%] absolute text-[#6D00FF] top-[75%] -left-[1.5%]
      animate-moving-up-and-down-10 [animation-duration:5s] md:hidden xl:block xl:top-[80%]' %}
      {% render 'icon', name: 'md-dollar', class: 'w-[4.5%] absolute text-[#6D00FF] top-[97%] left-[17%]
      animate-moving-down-and-up-10 [animation-duration:7s]' %}
      <div class="bg-[#CEA5FF] -z-10 rounded-[10%] w-[12%] aspect-square absolute top-[75%] left-[14%]
      animate-moving-down-and-up-10 [animation-duration:8s] shadow-[0px_15.343px_15.343px_-6.136px_rgba(50,8,106,0.28)]"></div>
      <div class="bg-[#7141AC] rounded-[10%] w-[7%] aspect-square absolute top-[93%] left-[4%]
      animate-moving-up-and-down-10 shadow-[0px_15.343px_15.343px_-6.136px_rgba(50,8,106,0.60)]"></div>
    </div>

    <div class="mt-[90px] md:mt-[103px] xl:mt-0 xl:w-[520px]">
      <h1 class="text-4xl font-bold bg-gr-1 bg-clip-text text-transparent md:text-[54px] md:leading-[1.18]">
        {{ section.settings.title }}
      </h1>
      <p class="mt-5 md:mt-[30px] md:text-xl md:leading-normal">{{ section.settings.subheading }}</p>

      <div class="mt-7 flex gap-4 flex-wrap items-center md:mt-10 md:gap-5">
        {%- assign button_icon = section.settings.button_icon -%}

        <a class="b-btn b-secondary-btn {% if button_icon != blank %}b-btn-with-icon{% endif %}" href="{{ section.settings.button_link }}">
          {%- if button_icon != blank -%}
            <img class="w-[18px] aspect-square mr-1.5" src="{{ button_icon | img_url }}" alt="">
          {%- endif -%}
          {{ section.settings.button_text }}
        </a>

        {%- if section.settings.show_shopify_badge -%}
          <div class="inline-flex items-end justify-center px-2 pt-1 pb-1.5 text-xs font-bold rounded-lg bg-[#E0F0FF]
          text-black leading-[1.1] md:text-sm">
            <img class="w-4 mr-1 md:w-[18px]" alt="" src="data:image/png;base64,">
            Built for Shopify
          </div>
        {%- endif -%}
      </div>
    </div>
  </div>

  {% comment %}
    Animation for the background image
  {% endcomment %}
  <div class="section-{{ section.id }}-bg-container">
